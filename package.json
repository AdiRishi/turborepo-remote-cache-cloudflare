{
  "name": "turborepo-remote-cache-cf",
  "version": "0.0.0",
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230419.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "miniflare": "^2.14.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0",
    "vitest-environment-miniflare": "^2.14.0",
    "wrangler": "^2.20.0"
  },
  "private": true,
  "scripts": {
    "build": "wrangler publish --dry-run --outdir dist",
    "dev": "wrangler dev --local --persist",
    "dev:scheduled": "wrangler dev --local --persist --test-scheduled",
    "test": "NODE_OPTIONS=--experimental-vm-modules vitest run --coverage",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules vitest",
    "lint": "eslint . --ext .ts,.js,.cjs,.mjs,.json --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.1.2",
    "hono": "^3.1.8",
    "zod": "^3.21.4"
  }
}
